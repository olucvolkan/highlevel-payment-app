APP_NAME="HighLevel PayTR Integration"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=highlevel_payments
DB_USERNAME=laravel
DB_PASSWORD=secret

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# PayTR Configuration
PAYTR_MERCHANT_ID=
PAYTR_MERCHANT_KEY=
PAYTR_MERCHANT_SALT=
PAYTR_TEST_MODE=1
PAYTR_API_URL=https://www.paytr.com

# HighLevel Configuration
HIGHLEVEL_CLIENT_ID=
HIGHLEVEL_CLIENT_SECRET=
HIGHLEVEL_SSO_KEY=
HIGHLEVEL_REDIRECT_URI="${APP_URL}/oauth/callback"
HIGHLEVEL_WEBHOOK_URL="${APP_URL}/webhooks/marketplace"
HIGHLEVEL_API_URL=https://backend.leadconnectorhq.com
HIGHLEVEL_OAUTH_URL=https://marketplace.gohighlevel.com

# Payment Provider Settings
DEFAULT_PAYMENT_PROVIDER=paytr
PAYMENT_CURRENCY=TRY
PAYMENT_SUCCESS_URL="${APP_URL}/payments/success"
PAYMENT_FAIL_URL="${APP_URL}/payments/error"
